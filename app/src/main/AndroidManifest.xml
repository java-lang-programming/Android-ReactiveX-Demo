<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="java_lang_programming.com.android_reactivex_demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.AsyncSubjectActivity" />
        <activity
            android:name=".ui.ObservableActivity"
            android:label="@string/title_activity_observable"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.ObservableMapActivity"
            android:label="@string/title_activity_observable_map"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>